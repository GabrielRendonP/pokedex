<p id="notice"><%= notice %></p>

<h1>Poke Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poke_types.each do |poke_type| %>
      <tr>
        <td><%= poke_type.name %></td>
        <td><%= link_to 'Show', poke_type %></td>
        <td><%= link_to 'Edit', edit_poke_type_path(poke_type) %></td>
        <td><%= link_to 'Destroy', poke_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poke Type', new_poke_type_path %>
